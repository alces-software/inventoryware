<%= @asset_data.name %>:
  name: <%= @asset_data.name %>
  primary_group: <%= @asset_data.mutable.primary_group %>
  secondary_group: <%= @asset_data.mutable.secondary_groups %>
  info: |
  <% @asset_hash['mutable']['maps'].each do |map| %>

    ## <%= map.first %>

    ### Port Layout

    <% map[1]['map'].each do |port, asset| %>
