<%= @node_data.name %>:
  name: <%= @node_data.name %>
  primary_group: <%= @node_data.mutable.primary_group %>
  secondary_group: <%= @node_data.mutable.secondary_groups %>
  info: |
    ## Port Layout

    <% @node_data.mutable.map.each do |port, node| %>
    <%= port %>: <%= node %>
    <% end %>
